{
  "name": "dflib",
  "version": "1.0.0",
  "description": "A simple TypeScript DataFrame library",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js", 
    "require": "./dist/index.js" 
  },  
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
